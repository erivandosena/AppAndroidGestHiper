<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.joaopaulo.gestanteautocuidadopa.Pag3"

    android:id="@+id/activity_Pag3">

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="pag4_1"
        android:text="Para saber mais!!!"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="pag4_2"
        android:text="O QUE É HIPERTENSÃO/ SÍNDROME HIPERTENSIVA?"
        app:layout_constraintTop_toBottomOf="@+id/button31"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="pag4_3"
        android:text="O QUE VOCÊ DEVE SABER ANTES DE AFERIR SUA PRESSÃO???"
        app:layout_constraintTop_toBottomOf="@+id/button32"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="pag4_4"
        android:text="DIÁRIO DE PRESSÃO ARTERIAL"
        app:layout_constraintTop_toBottomOf="@+id/button33"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="pag4_5"
        android:text="EXERCÍCIO FÍSICO NA GRAVIDEZ"
        app:layout_constraintTop_toBottomOf="@+id/button34"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="pag4_6"
        android:text="ALIMENTAÇÃO SAUDÁVEL NA GRAVIIDEZ"
        app:layout_constraintTop_toBottomOf="@+id/button35"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="pag4_7"
        android:text="GALERIA DE FOTOS DA GRAVIDEZ"
        app:layout_constraintTop_toBottomOf="@+id/button36"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:onClick="voltarPag"
        android:text="voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="16dp" />

</android.support.constraint.ConstraintLayout>
